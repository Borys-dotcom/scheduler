{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "declaration": true,
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src"]
}