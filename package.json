{
  "name": "day-week-simple-schedule",
  "version": "1.3.1",
  "description": "A custom React Scheduler component",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:ts && npm run copy:css",
    "build:ts": "tsc -p .",
    "copy:css": "cpx \"./src/**/*.css\" dist/",
