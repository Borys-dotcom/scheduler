    "start": "ts-node src/index.tsx"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ]
}
